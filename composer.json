{
    "name": "phlib/beanstalk",
    "description": "Library for handling beanstalk connections.",
    "type": "library",
    "keywords": ["beanstalk", "pool", "job", "queue"],
    "license": "LGPL-3.0",
    "homepage": "https://github.com/phlib/beanstalk",
    "authors": [
        {
            "name": "James Dempster"
        },
        {
            "name": "Luke Richards"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/beanstalk/contributors"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "phlib/console-configuration": "^2",
        "psr/log": "^1 || ^2 || ^3",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "colinodell/psr-testlogger": "^1",
        "phpunit/phpunit": "^9",
        "php-mock/php-mock-phpunit": "^2",
        "symplify/easy-coding-standard": "^12"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\Beanstalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\Beanstalk\\": "tests/"
        }
    },
    "suggest": {
        "psr/log-implementation": "Optional Logger can capture connection failures"
    },
    "bin": ["bin/beanstalk"],
    "scripts": {
        "cs-check": "vendor/bin/ecs",
        "cs-fix": "vendor/bin/ecs --fix"
    }
}
