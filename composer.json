{
    "name": "phlib/beanstalk",
    "description": "Library for handling beanstalk connections.",
    "type": "library",
    "keywords": ["beanstalk", "pool", "job", "queue"],
    "license": "LGPL-3.0",
    "homepage": "https://github.com/phlib/beanstalk",
    "authors": [
        {
            "name": "James Dempster"
        },
        {
            "name": "Luke Richards"
        },
        {
            "name": "Phlib Team & Contributors",
            "homepage": "https://github.com/phlib/beanstalk/contributors"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "symfony/console": "^2.7|^3|^4|^5",
        "phlib/console-configuration": "^1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4",
        "php-mock/php-mock-phpunit": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Phlib\\Beanstalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phlib\\Beanstalk\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/beanstalk"]
}
